---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>cestlaz.github.com/minutia</title>
  <link href="http://cestlaz.github.com/minutia/"/>
  <link type="application/atom+xml" rel="self" href="http://cestlaz.github.com/minutia/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://cestlaz.github.com/minutia/</id>
  <author>
    <name>Mike Zamansky</name>
    <email>zamnasky@stuycs.org</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://cestlaz.github.com/minutia{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://cestlaz.github.com/minutia{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>C'est la Z</name>
      <uri>http://cestlaz.github.com/minutia/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
